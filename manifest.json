{
  "manifest_version": 2,
  "name": "GitHub Stars Dashboard",
  "version": "1.0",
  "description": "Adds your GitHub stars to the dashboard.",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/", "*://github.com/dashboard/*"],
      "js": ["main.js"]
    }
  ]
}